{
  "expo": {
    "name": "signacalc",
    "slug": "signacalc",
    "description": "An easy to use calculator at your finger tip",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/calclogoicon.png",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.signacode.signacalc"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/calclogoicon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/calclogoicon.png"
    }
    
    
  }
}
